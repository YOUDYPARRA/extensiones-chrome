{
	"name":"Beber agua Event PopUp",
	"description":"Demuestra el evento agua de la página recordando a usuario beber agüita de calzón",
	"version":"1.0",
	"manifest_version":2,
	"permissions":["alarms","notifications","storage"],
	"background":{
		"scripts":["background.js"],
		"persistent":false
	},
	"browser_action":{
		"default_title":"Evento beber agua de calzón",
		"default_popup":"popup.html"

	},
	"icons":{
		"16":"img/drink_water16.png",
		"32":"img/drink_water32.png",
		"48":"img/drink_water48.png",
		"128":"img/drink_water128.png"
	}


}